{
    "name": "skukunin/database-activity-bundle",
    "description": "Middleware for storing entity changes with simple interface for admins implemented as Symfony bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Serhii Kukunin",
            "email": "serhii.kukunin@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3|^8.0|^8.1",
        "doctrine/doctrine-bundle": "^2.7",
        "symfony/dependency-injection": ">=4.4",
        "symfony/config": ">=4.4",
        "doctrine/orm": "^2.13",
        "doctrine/dbal": "^3.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.1",
        "spatie/phpunit-watcher": "^1.23",
        "symfony/security-bundle": ">=4.4"
    },
    "autoload": {
        "psr-4": {
            "SKukunin\\DatabaseActivityBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SKukunin\\DatabaseActivityBundle\\Tests\\": "tests/"
        }
    }
}
